{
  "name": "frontend",
  "version": "1.0.0-beta.9",
  "description": "",
  "main": "dist",
  "scripts": {
    "test" : "npm run test:back && npm run test:front",
    "test:back:tdd" : "npm run test:back -- --watch",
    "test:front:tdd" : "npm run test:front -- --watch",
    "test:front": "jest --config ./src/frontend/jest.config.json --runInBand",
    "test:back": "jest --config ./src/backend/jest.config.json --runInBand",
    "start:back": "nodemon --watch './src/backend/**/*.ts' --ignore './src/backend/**/*.spec.ts' --exec 'ts-node' ./src/backend/index.ts < /dev/null",
    "setup-test-db": "ts-node src/backend/scripts/eveStaticDataImport/bin.ts -- --db=TEST_DB",
    "update-static-data": "ts-node src/backend/scripts/eveStaticDataImport/bin.ts --db=eveonline",
    "start:dev": "NODE_ENV=development npm run start:front & NODE_ENV=development HYDRATE_ON_START=0 npm run start:back",
    "start": "NODE_ENV=production npm run start:front & NODE_ENV=production npm run start:back",
    "build:front": "NODE_ENV=production webpack -c ./projectConfig/webpack.config.js",
    "start:front": "webpack serve -c ./projectConfig/webpack.devserver.config.js"
  },
  "keywords": [],
  "author": "Patrick Tolosa",
